<?php
namespace App\Enums;

enum {{ CLASS }}: int
{
    case Option = 1;

    public function label(): string
    {
        return match($this)
        {
            self::Option => 'label',

            default => 'Indefinido'
        };
    }

    public function color(): string
    {
        return match($this)
        {
            self::Option => 'success',

            default => 'light'
        };
    }
}